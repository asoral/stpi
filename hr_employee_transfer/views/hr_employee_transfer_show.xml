<odoo>
<record id="employeetransfer_show_tree_view" model="ir.ui.view">
            <field name="name">employee.transfer.tree</field>
            <field name="model">hr.employee.transfer</field>
            <field name="arch" type="xml">
                <tree string="HR Employee Transfer" create="0">
                    <field name="employee_id"/>
                    <field name="transfer_to"/>
                    <field name="file_number"/>
                    <field name="order_number"/>
                    <field name="order_date"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
</odoo>